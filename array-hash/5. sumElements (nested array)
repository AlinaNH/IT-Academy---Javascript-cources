<html>
  <head>
    <title>Sum of elements (nested array)</title>
    <script>
      var nestedArray = [ 5, 7, [ 4, [2], 8, [1,3], 2 ], [ 9, [] ], 1, 8 ];
      var sum = 0;
      function treesum(array) {
          for (var i = 0; i < array.length; i++) {
              if (typeof(array[i]) != "object") {
                  sum += array[i];
              } else {
                  treesum(array[i]);
              }
          }
          return sum;
      }

      console.log( treesum(nestedArray) );
    </script>
  </head>
</html>
